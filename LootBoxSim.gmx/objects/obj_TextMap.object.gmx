          <argument>
            <kind>1</kind>
            <string>var input = global.input

if input!=""{
    input=scr_Synonym(input)
    
    lastx=curx
    lasty=cury
    
    if(string_copy(input,1,2)="go"){
        var movex=0
        var movey=0
        if string_copy(input,3,1)="n"{
            if move[0] movey=-1
            else message("You have not unlocked the ability to move North yet.",false)
        }
        if string_copy(input,3,1)="s"{
            if move[1] movey=1
            else message("You have not unlocked the ability to move South yet.",false)
        }
        if string_copy(input,3,1)="w"{
            if move[2] movex=-1
            else message("You have not unlocked the ability to move West yet.",false)
        }
        if string_copy(input,3,1)="e"{
            if move[3] movex=1
            else message("You have not unlocked the ability to move East yet.",false)
        }
        curx+=movex
        cury+=movey
        if(curx&lt;0||cury&lt;0||curx &gt;= array_length_2d(spot,cury)|| cury &gt;= array_height_2d(spot)){
            curx=lastx
            cury=lasty
            message("You cannot go that way.",true)
        }
        else message(spot[cury,curx],false)
    }
    else if(input="help"){
        message("Open Lootbox - Open one of your Lootboxes and get the reward inside!",true)
        message("Go - Move either North, South, East, or West", true)
        message("Grab - Pick up an object", true)
        message("Look - Observe your surroundings again", true) 
        message("Talk - talk to a person", true)
        message("Color - change the color of the text", true);
    }
    else if(string_copy(input,1,5) = "color"){
     global.text_color = real(string_copy(input,6,9));
    }else if (string_copy(input,1,4) = "talk") {
     var name = string_copy(input,5,10);
     var map = grid[# curx, cury];
     
     if (!is_undefined(map[? name])) {
      message(map[? name], false);
     } else {
      message("That person isn't here!", true);
     }
     
    }
    else if(input="look") message(spot[cury,curx], false)
    else if(input="fuck") message(choose("Hey, that's rude!", "Watch your language!", "No swearing on my christian game.", "At least invite me to dinner first!"), true);
    else if(input="piss") message("Uhm...no?", true); 
    else message(choose("What?","Huh?","I don't understand.","That doesn't make sense","What do you mean?","That's nonsense.","I don't think I can do that.","I grasp very few concepts, that is not one of them.","Nobody has ever done that, nope.","No.","That's probably impossible","Nah."),true);
    
    
    global.input=""

}



