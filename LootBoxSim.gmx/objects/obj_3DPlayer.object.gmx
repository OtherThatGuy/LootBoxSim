            <string>if(keyboard_check(vk_up)||keyboard_check(ord("W"))){
    var vect_x = lengthdir_x(1, target_dir)
    var vect_y = lengthdir_y(1, target_dir)
    
    if (!place_meeting(target_x+vect_x,target_y+vect_y,obj_3DWall)){
        target_x+=vect_x
        target_y+=vect_y
    }
}
if(keyboard_check(vk_down)||keyboard_check(ord("S"))){
    var vect_x = lengthdir_x(1, target_dir)
    var vect_y = lengthdir_y(1, target_dir)
    
    if (!place_meeting(target_x-vect_x,target_y-vect_y,obj_3DWall)){
        target_x-=vect_x
        target_y-=vect_y
    }
}

if(keyboard_check(ord("D"))){
    var vect_x = lengthdir_x(1, target_dir+90)
    var vect_y = lengthdir_y(1, target_dir+90)
    
    if (!place_meeting(target_x-vect_x,target_y-vect_y,obj_3DWall)){
        target_x-=vect_x
        target_y-=vect_y
    }
}
if(keyboard_check(ord("A"))){
    var vect_x = lengthdir_x(1, target_dir-90)
    var vect_y = lengthdir_y(1, target_dir-90)
    
    if (!place_meeting(target_x-vect_x,target_y-vect_y,obj_3DWall)){
        target_x-=vect_x
        target_y-=vect_y
    }
}

if mouselock{
    window_set_cursor(cr_none)
    target_dir+=((window_get_width()/2)-mouse_x)/6
    dir+=((window_get_width()/2)-mouse_x)/6
    
    z_angle+=((window_get_height()/2)-mouse_y)/12
    z_angle=(clamp(z_angle,-45,45))
    
    window_mouse_set(window_get_width()/2,window_get_height()/2)
}
else{
    window_set_cursor(cr_default)
    z_angle=lerp(z_angle,0,0.1)
}

if(keyboard_check(vk_right)) target_dir-=1
if(keyboard_check(vk_left)) target_dir+=1


if os_browser=browser_not_a_browser{
    if keyboard_check_pressed(vk_escape) mouselock=!mouselock
}
else mouselock=false
</string>
